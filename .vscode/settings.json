{
  "files.eol": "\n",
  "editor.tabSize": 2,
  "eslint.format.enable": true,
  "less.lint.unknownAtRules": "ignore",
  "css.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "eslint.experimental.useFlatConfig": true,
  "i18n-ally.enabledParsers": ["json", "ts"], //这个最好加上，如果是其他格式，如ts，不加上就无效了
  "i18n-ally.keystyle": "auto", // 翻译路径格式,
  "i18n-ally.sourceLanguage": "en", // 翻译源语言
  "i18n-ally.displayLanguage": "zh-CN", //显示语言， 这里也可以设置显示英文为en,
  "i18n-ally.localesPaths": ["src/locale"]
}
